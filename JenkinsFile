pipeline
{
       agent any
       tools
       {
         maven 'MAVEN_HOME'
         
         }
       stages{
       stage('Welcome Stage')
       {
       steps
       {
       echo 'Welcome to Jenkins Pipeline'
       }
       }
       stage('Build Stage')
       {
       steps
       {
       bat 'mvn install'
       }
       }
       stage('Test Stage')
       {
       steps
       {
       bat 'mvn test'
       }
       }
       stage('Success Stage')
       {
       steps 
       {
       echo 'Successfully Build'
       }
       }
}
}